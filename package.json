{
  "name": "todos",
  "version": "1.0.0",
  "description": "Todo Assignment",
  "main": "src/index.js",
  "scripts": {
    "build": "yarn run clean && yarn run build:client && yarn run build:server",
    "build:client": "NODE_ENV=production BABEL_ENV=production node -r babel-register node_modules/.bin/webpack --progress --display-modules --config webpack/client.babel.js",
    "build:server": "NODE_ENV=production BABEL_ENV=production node -r babel-register node_modules/.bin/webpack --progress --config webpack/server.babel.js --debug",
    "browsers": "yarn browserslist",
    "clean": "rimraf dist/ && rimraf public/app",
    "flow": "flow --show-all-errors",
    "lint": "eslint *.js .storybook/ flow/ src/ test/ webpack/",
    "lint:check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint:fix": "yarn run lint --fix",
    "postinstall": "flow-typed install",
    "storybook": "start-storybook -p 8081 -c .storybook",
    "start": "yarn run clean && NODE_ENV=development BABEL_ENV=development NODE_PORT=8080 babel-node src/server",
    "test": "jest",
    "watch:client": "webpack --watch --config webpack/client.babel.js",
    "watch:server": "webpack --watch --config webpack/server.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/robberthalff/todos.git"
  },
  "keywords": [
    "Todo",
    "Upwork",
    "Assignment"
  ],
  "author": "Rob Halff <rob.halff@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robberthalff/todos/issues"
  },
  "homepage": "https://github.com/robberthalff/todos#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "node-time-ago": "^1.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-actions": "^2.3.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.4.1",
    "autoprefixer": "^8.3.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-react-flow-props-to-prop-types": "^0.15.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.11",
    "css-modules-flow-types-cli": "^1.1.0",
    "css-modules-flow-types-loader": "^1.1.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-flowtype": "^2.46.2",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.70.0",
    "flow-typed": "^2.4.0",
    "flow-types": "^1.0.0",
    "gh-pages": "^1.1.0",
    "glob": "^7.1.2",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-style-loader": "^4.0.0",
    "jest": "^22.4.3",
    "json-loader": "^0.5.7",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.4",
    "prettier": "^1.12.1",
    "prop-types": "^15.6.1",
    "prop-types-extra": "^1.0.1",
    "react-hot-loader": "^4.0.1",
    "react-styleguidist": "^7.0.5",
    "react-test-renderer": "^16.3.2",
    "redux-devtools-extension": "^2.13.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.3",
    "svg-sprite-loader": "^3.7.3",
    "svgo-loader": "^2.1.0",
    "url-loader": "^1.0.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/webpack/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "roots": [
      "<rootDir>/src/"
    ]
  }
}
